<!-- script to calculate BuyBack -->
<script>
document.getElementById('fabuybacksBB').innerHTML = "0";
document.getElementById('retailpaidoutBB').innerHTML = "0";
document.getElementById('wholesalepaidoutBB').innerHTML = "0";
document.getElementById('paidoutBB').innerHTML = "0";

function calculateBB() {

var registernumberFieldBB = document.getElementById('registernumberBB').value;
document.getElementById('buttonBB').innerHTML = "Buyback Register"  + " " + registernumberFieldBB;

var cashandcoinreceiptFieldBB = document.getElementById('cashandcoinreceiptBB').value;
var checksFieldBB = document.getElementById('checksBB').value;
var discoverFieldBB = document.getElementById('discoverBB').value;
var financialaidFieldBB = document.getElementById('financialaidBB').value;
var manualcreditcardFieldBB = document.getElementById('manualcreditcardBB').value;
var mastercard1FieldBB = document.getElementById('mastercard1BB').value;
var mastercard2FieldBB = document.getElementById('mastercard2BB').value;
var squareFieldBB = document.getElementById('squareBB').value;
var visa1FieldBB = document.getElementById('visa1BB').value;
var visa2FieldBB = document.getElementById('visa2BB').value;

var totalreceiptsSumBB = parseFloat(cashandcoinreceiptFieldBB) + parseFloat(checksFieldBB) + parseFloat(financialaidFieldBB) + parseFloat(checksFieldBB) 
+ parseFloat(discoverFieldBB) + parseFloat(manualcreditcardFieldBB) + parseFloat(mastercard1FieldBB) + parseFloat(mastercard2FieldBB) 
+ parseFloat(squareFieldBB) + parseFloat(visa1FieldBB) + parseFloat(visa2FieldBB);
totalreceiptsBB.textContent = totalreceiptsSumBB.toFixed(2);

var salestax1FieldBB = document.getElementById('salestax1BB').value;
var salestax2FieldBB = document.getElementById('salestax2BB').value;
var efolletttaxFieldBB = document.getElementById('efolletttaxBB').value;
var purchasedgiftcertFieldBB = document.getElementById('purchasedgiftcertBB').value;

var lessSumBB = parseFloat(salestax1FieldBB) + parseFloat(salestax2FieldBB) + parseFloat(efolletttaxFieldBB) + parseFloat(purchasedgiftcertFieldBB);
lessBB.textContent = lessSumBB.toFixed(2);

var redeemedgiftcertFieldBB = document.getElementById('redeemedgiftcertBB').value;
var onaccountFieldBB = document.getElementById('onaccountBB').value;

var plusSumBB = parseFloat(purchasedgiftcertFieldBB) + parseFloat(redeemedgiftcertFieldBB) + parseFloat(onaccountFieldBB);
plusBB.textContent = plusSumBB.toFixed(2);

var netsalesSumBB = (parseFloat(cashandcoinreceiptFieldBB) + parseFloat(checksFieldBB) + parseFloat(financialaidFieldBB) + parseFloat(checksFieldBB) 
+ parseFloat(discoverFieldBB) + parseFloat(manualcreditcardFieldBB) + parseFloat(mastercard1FieldBB) + parseFloat(mastercard2FieldBB) 
+ parseFloat(squareFieldBB) + parseFloat(visa1FieldBB) + parseFloat(visa2FieldBB)) - (parseFloat(salestax1FieldBB) + parseFloat(salestax2FieldBB) + 
parseFloat(efolletttaxFieldBB)) + (parseFloat(purchasedgiftcertFieldBB) + parseFloat(redeemedgiftcertFieldBB) + parseFloat(onaccountFieldBB));

netsalesBB.textContent = netsalesSumBB.toFixed(2);

var totaltransactionsFieldBB = document.getElementById('totaltransactionsBB').value;
var rentalincomeFieldBB = document.getElementById('rentalincomeBB').value;
var paidoutFieldBB = document.getElementById('paidoutBB').value * -1;

var retailpaidoutFieldBB = document.getElementById('retailpaidoutBB').value;
var wholesalepaidoutFieldBB = document.getElementById('wholesalepaidoutBB').value;

var cashpaidoutSumBB = (parseFloat(retailpaidoutFieldBB) + parseFloat(wholesalepaidoutFieldBB)) * -1;
cashpaidoutBB.textContent = cashpaidoutSumBB.toFixed(2);

/* indrawer is an arbitrary total of original paidout(paidin) minus buyback report */
var indrawerSumBB = parseFloat(paidoutFieldBB) - (parseFloat(cashpaidoutSumBB) * -1);
indrawerBB.textContent = indrawerSumBB.toFixed(2);

/* actualcash is the physical amount of money in the bag (which includes the paidout(paidin) from the start) */
var actualcashFieldBB = document.getElementById('actualcashBB').value;


if (cashandcoinreceiptFieldBB < 0) {
var pctransferSumBB = cashandcoinreceiptFieldBB;
pctransferBB.textContent = parseFloat(pctransferSumBB);
}
else {
pctransferBB.textContent = "0.00";
}


/* totaldepositsSum is for the field in the middle of the receipt */
var totaldepositsSumBB = (parseFloat(cashandcoinreceiptFieldBB) + parseFloat(financialaidFieldBB) + parseFloat(checksFieldBB) 
+ parseFloat(discoverFieldBB) + parseFloat(manualcreditcardFieldBB) + parseFloat(mastercard1FieldBB) + parseFloat(mastercard2FieldBB) 
+ parseFloat(squareFieldBB) + parseFloat(visa1FieldBB) + parseFloat(visa2FieldBB)) + parseFloat(paidoutFieldBB);
totaldepositsBB.textContent = totaldepositsSumBB.toFixed(2);


/* totalcountedSum will calculate money earned including the physical cash from the bag, 
plus the cashpaidout(paidin) amount the drawer started with, minus the buyback report total */
var checksbagFieldBB = document.getElementById('checksbagBB').value;
var totalcountedSumBB = parseFloat(actualcashFieldBB) + parseFloat(checksbagFieldBB) + parseFloat(financialaidFieldBB) 
+ parseFloat(discoverFieldBB) + parseFloat(manualcreditcardFieldBB) + parseFloat(mastercard1FieldBB) + parseFloat(mastercard2FieldBB) 
+ parseFloat(squareFieldBB) + parseFloat(visa1FieldBB) + parseFloat(visa2FieldBB);
totalcountedBB.textContent = totalcountedSumBB.toFixed(2);

/* totalfromreceiptSum will calculate money earned including cash from receipt, 
plus the cashpaidout amount the drawer started with, minus the buyback report total */

var totalfromreceiptSumBB =  (parseFloat(cashandcoinreceiptFieldBB) + parseFloat(financialaidFieldBB) + parseFloat(checksFieldBB) 
+ parseFloat(discoverFieldBB) + parseFloat(manualcreditcardFieldBB) + parseFloat(mastercard1FieldBB) + parseFloat(mastercard2FieldBB) 
+ parseFloat(squareFieldBB) + parseFloat(visa1FieldBB) + parseFloat(visa2FieldBB) + parseFloat(paidoutFieldBB))
- (parseFloat(retailpaidoutFieldBB) + parseFloat(wholesalepaidoutFieldBB));
totalfromreceiptBB.textContent = totalfromreceiptSumBB.toFixed(2);


var overshortSumBB = totalcountedSumBB - totalfromreceiptSumBB;
overshortBB.textContent = overshortSumBB.toFixed(2);

}

var calculateButtonBB = document.getElementById('calculateButtonBB');
calculateButtonBB.addEventListener('click', calculateBB, false);

</script>


