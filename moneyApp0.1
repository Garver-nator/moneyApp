parseFloat(mastercard2FieldB) + parseFloat(squareFieldB) + parseFloat(visa1FieldB) + parseFloat(visa2FieldB);
totalreceiptsB.textContent = totalreceiptsSumB.toFixed(2);

var salestax1FieldB = document.getElementById('salestax1B').value;
