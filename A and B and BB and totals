<!-- script to calculate B -->
<script>
function calculateB() {

var cashandcoinreceiptFieldB = document.getElementById('cashandcoinreceiptB').value;
var checksFieldB = document.getElementById('checksB').value;
var discoverFieldB = document.getElementById('discoverB').value;
var financialaidFieldB = document.getElementById('financialaidB').value;
var manualcreditcardFieldB = document.getElementById('manualcreditcardB').value;
var mastercard1FieldB = document.getElementById('mastercard1B').value;
var mastercard2FieldB = document.getElementById('mastercard2B').value;
var visa1FieldB = document.getElementById('visa1B').value;
var visa2FieldB = document.getElementById('visa2B').value;

var totalreceiptsSumB = parseFloat(cashandcoinreceiptFieldB) + parseFloat(checksFieldB) + parseFloat(financialaidFieldB) + parseFloat(checksFieldB) 
+ parseFloat(discoverFieldB) + parseFloat(manualcreditcardFieldB) + parseFloat(mastercard1FieldB) + parseFloat(mastercard2FieldB) 
+ parseFloat(visa1FieldB) + parseFloat(visa2FieldB);
totalreceiptsB.textContent = totalreceiptsSumB.toFixed(2);

var salestax1FieldB = document.getElementById('salestax1B').value;
var salestax2FieldB = document.getElementById('salestax2B').value;
var efolletttaxFieldB = document.getElementById('efolletttaxB').value;
var purchasedgiftcertFieldB = document.getElementById('purchasedgiftcertB').value;

var lessSumB = parseFloat(salestax1FieldB) + parseFloat(salestax2FieldB) + parseFloat(efolletttaxFieldB) + parseFloat(purchasedgiftcertFieldB);
lessB.textContent = lessSumB.toFixed(2);

var redeemedgiftcertFieldB = document.getElementById('redeemedgiftcertB').value;
var onaccountFieldB = document.getElementById('onaccountB').value;

var plusSumB = parseFloat(purchasedgiftcertFieldB) + parseFloat(redeemedgiftcertFieldB) + parseFloat(onaccountFieldB);
plusB.textContent = plusSumB.toFixed(2);

var netsalesSumB = (parseFloat(cashandcoinreceiptFieldB) + parseFloat(checksFieldB) + parseFloat(financialaidFieldB) + parseFloat(checksFieldB) 
+ parseFloat(discoverFieldB) + parseFloat(manualcreditcardFieldB) + parseFloat(mastercard1FieldB) + parseFloat(mastercard2FieldB) 
+ parseFloat(visa1FieldB) + parseFloat(visa2FieldB)) - (parseFloat(salestax1FieldB) + parseFloat(salestax2FieldB) + 
parseFloat(efolletttaxFieldB)) + (parseFloat(purchasedgiftcertFieldB) + parseFloat(redeemedgiftcertFieldB) + parseFloat(onaccountFieldB));

netsalesB.textContent = netsalesSumB.toFixed(2);

var totaltransactionsFieldB = document.getElementById('totaltransactionsB').value;
var rentalincomeFieldB = document.getElementById('rentalincomeB').value;
var paidoutFieldB = document.getElementById('paidoutB').value;
var cashandcoinbagFieldB = document.getElementById('cashandcoinbagB').value;

var totalcountedSumB = (parseFloat(cashandcoinbagFieldB) + parseFloat(financialaidFieldB)  
+ parseFloat(discoverFieldB) + parseFloat(manualcreditcardFieldB) + parseFloat(mastercard1FieldB) + parseFloat(mastercard2FieldB) 
+ parseFloat(visa1FieldB) + parseFloat(visa2FieldB));
totalcountedB.textContent = totalcountedSumB.toFixed(2);

var totaldepositsSumB = (parseFloat(cashandcoinreceiptFieldB) + parseFloat(financialaidFieldB) + parseFloat(checksFieldB) 
+ parseFloat(discoverFieldB) + parseFloat(manualcreditcardFieldB) + parseFloat(mastercard1FieldB) + parseFloat(mastercard2FieldB) 
+ parseFloat(visa1FieldB) + parseFloat(visa2FieldB)) + parseFloat(paidoutFieldB);
totaldepositsB.textContent = totaldepositsSumB.toFixed(2);

totalfromreceiptB.textContent = totaldepositsSumB.toFixed(2);

var overshortSumB = totalcountedSumB - totaldepositsSumB;
overshortB.textContent = overshortSumB.toFixed(2);

}

var calculateButtonB = document.getElementById('calculateButtonB');
calculateButtonB.addEventListener('click', calculateB, false);
</script>


