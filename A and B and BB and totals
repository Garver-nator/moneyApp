<!-- script for Total Daily Cash Analysis -->
<script>
document.getElementById('overshortA').innerHTML = "0";
document.getElementById('overshortB').innerHTML = "0";
document.getElementById('overshortBB').innerHTML = "0";
document.getElementById('totExpectedCashDeposit').innerHTML = "0";
document.getElementById('totCashToBeDeposited').innerHTML = "0";
document.getElementById('totOverShortForDay').innerHTML = "0";
document.getElementById('totLessBuyback').innerHTML = "0";
document.getElementById('totAdjustedCashDeposit').innerHTML = "0";
document.getElementById('totActualCashDeposit').innerHTML = "0";
document.getElementById('totPreviousReimbursementShortage').innerHTML = "0";


function calculateTotals() {

/* line 1 variables*/
var cashandcoinreceiptAField = parseFloat(document.getElementById('cashandcoinreceiptA').value);
var cashandcoinreceiptBField = parseFloat(document.getElementById('cashandcoinreceiptB').value);
var cashandcoinreceiptBBField = parseFloat(document.getElementById('cashandcoinreceiptBB').value);

/* line 1 calculation */
var totCashToBeDepositedField = cashandcoinreceiptAField + cashandcoinreceiptBField + cashandcoinreceiptBBField; 
/* line 1 post answer */
totCashToBeDeposited.textContent = totCashToBeDepositedField.toFixed(2);

/* line 2 variables */
var totLessCashShortageField = document.getElementById('totLessCashShortage').value;

/* line 3 variables*/
var overshortAField = document.getElementById('overshortA').innerHTML;
var overshortBField = document.getElementById('overshortB').innerHTML;
var overshortBBField = document.getElementById('overshortBB').innerHTML;
/* line 3 calculation */
var totOverShortForDayField = parseFloat(overshortAField) + parseFloat(overshortBField) + parseFloat(overshortBBField);
/* line 3 post answer */
totOverShortForDay.textContent = totOverShortForDayField.toFixed(2);

/*line 4 variables (listed above) */
/* line 4 calculation*/
var totExpectedCashDepositField = parseFloat(totCashToBeDepositedField) + parseFloat(totLessCashShortageField) + parseFloat(totOverShortForDayField);
/*line 4 post answer */
totExpectedCashDeposit.textContent = totExpectedCashDepositField.toFixed(2);

/* line 6  is a repeat of line 4 */
totExpectedCashDeposit2.textContent = totExpectedCashDepositField;

/* line 7  variables */
var totLessBuybackField2 = document.getElementById('cashpaidoutBB').innerHTML;
